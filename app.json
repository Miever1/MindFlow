{
  "expo": {
    "name": "MindFlow",
    "slug": "MindFlow",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/appstore.png",
    "scheme": "mindflow",
    "userInterfaceStyle": "automatic",
    "ios": {
      "icon": "./assets/images/appstore.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.miever.MindFlow",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/1024.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.miever.MindFlow"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3bc0eec7-37c7-409e-85d9-4bb1f610c82b"
      }
    },
    "owner": "miever",
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/3bc0eec7-37c7-409e-85d9-4bb1f610c82b"
    }
  }
}
